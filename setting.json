{
	"backup": false,
	"mqtt" : {
		"topic": "flow/publish",
		"connection": {
			"host": "endpoint.iot.ap-northeast-1.amazonaws.com",
			"port": 8883,
			"ssl": true,
			"cert_file": "cert.pem",
			"key_file": "thing-private-key.pem",
			"ca_file": "rootCA.pem"
		}
	},
	"node-red": {
		"flows" : "/home/pi/.node-red/flows_raspberrypi.json",
		"start" : "sudo systemctl start nodered",
		"stop" : "sudo systemctl stop nodered",
		"interval": 1
	}
}

